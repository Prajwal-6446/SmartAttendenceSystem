@page
@model Biometric_Attendence1.Pages.MDelete1Model
@if (TempData["Success"] != null)
{
	<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" />
	<script type="text/javascript">
		toastr.success('@TempData["Success"]');
	</script>
}


<form method="post">
	<input hidden asp-for="ManageUser.Id" />
	<div class="border p-3 mt-4">
		<div class="row pb-2">
			<h2 class="text-primary pl-3">Delete Category</h2>
			<hr />
		</div>
		<div class="mb-3">
			<label asp-for="ManageUser.Name"></label>
			<input asp-for="ManageUser.Name" disabled class="form-control" />
		</div>
		<div class="mb-3">
			<label asp-for="ManageUser.FingerId"></label>
			<input asp-for="ManageUser.FingerId" disabled class="form-control" />
		</div>
		<button type="submit" class="btn btn-danger" style="width:150px;">Delete</button>
		<a asp-page="MIndex1" class="btn btn-secondary" style="width:150px;">Back to List</a>
	</div>
</form>

@section Scripts {
	@{
		<partial name="_ValidationScriptsPartial" />
	}
}
